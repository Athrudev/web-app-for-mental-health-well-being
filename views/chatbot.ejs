    <title>Dr. Mine!</title>
    <style>
      body {
        margin: 0;
        padding: 0;
        display: flex;
        flex-wrap: wrap;
        justify-content: space-around;
        align-items: center;
        min-height: 100vh;
      }

      .container {
        opacity: 40%;
        width: calc(30% - 90px);
        height: 14vh;
        padding: 68px;
        
        position: relative;
        /*box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;*/
      }

      .container img {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%; /* Ensure GIFs fill the container */
        opacity: 60; /* Initially hide all GIFs */
        transition: opacity 5s ease-in-out; /* Smooth transition for image swap */
      }

      /* Animation for looping GIFs in each container */
      .container img:first-child {
        animation: gif-loop-1 10s infinite ease-in-out;
      }
      .container img:nth-child(2) {
        animation: gif-loop-2 10s infinite ease-in-out;
        animation-delay: 10s;
      } /* Delay by animation duration */
      .container img:nth-child(3) {
        animation: gif-loop-3 10s infinite ease-in-out;
        animation-delay: 10s;
      } /* Delay by combined duration */

      @keyframes gif-loop-1 {
        0% {
          opacity: 1;
        }
        33% {
          opacity: 1;
        }
        66% {
          opacity: 0;
        }
        100% {
          opacity: 0;
        }
      }

      @keyframes gif-loop-2 {
        0% {
          opacity: 0;
        }
        33% {
          opacity: 0;
        }
        66% {
          opacity: 1;
        }
        100% {
          opacity: 1;
        }
      }

      @keyframes gif-loop-3 {
        0% {
          opacity: 0;
        }
        66% {
          opacity: 0;
        }
        100% {
          opacity: 1;
        }
      }
    </style>
  </head>
